{
  "name": "b2b-eshop-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/seed.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"prisma/seed.ts\"",
    "pre-commit": "npm run lint && npm run format:check && npm run build",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "We Are Lootea s. r. o.",
  "license": "UNLICENSED",
  "type": "module",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@prisma/client": "^6.10.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.4.0",
    "graphql": "^16.8.1",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "prisma": "^6.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "graphql-request": "^6.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
